{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.38",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prepare": "ts-node ../shared/scripts/setupBaseInstallation.ts && npm run copy-shared && npm run ensure-dev-api",
    "ensure-dev-api": "ts-node ../shared/scripts/checkAPIStartedForDev.ts",
    "copy-shared": "npx ts-node ../shared/scripts/copySharedFiles.ts --project frontend",
    "start": "BROWSER=none PORT=3001 react-scripts start",
    "build:ci": "npm run prepare && npm install && react-scripts build",
    "test:component": "npm run prepare && jest -c jest.config.js",
    "test:dev": "npm run prepare && react-scripts test",
    "test:e2e:dev": "npm run prepare && jest -c jest.config.e2e.js --watch --verbose true",
    "test:e2e:dev:fast": "jest -c jest.config.e2e.js --watch --verbose true",
    "test:e2e": "npm run prepare && jest -c jest.config.e2e.js",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.2",
    "@types/puppeteer": "^7.0.4",
    "axios": "^1.4.0",
    "concurrently": "^8.2.0",
    "dotenv": "^16.3.1",
    "jest": "^29.6.1",
    "jest-cucumber": "^3.0.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-puppeteer": "^9.0.0",
    "jest-transform-css": "^6.0.1",
    "ndb": "^1.1.5",
    "puppeteer": "^20.7.4",
    "react-hooks-global-state": "^2.1.0",
    "react-router-dom": "^6.14.1",
    "react-toastify": "^9.1.3",
    "rxjs": "^7.8.1",
    "start-server-and-test": "^2.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1"
  }
}
